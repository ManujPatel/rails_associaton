<% if flash[:errors] %>
      <ul>
        <% flash[:errors].each do |error| %>
          <li style="color:red;"><%= error %></li>
        <% end %>
      </ul>
<% end %>
<% if flash[:notice] %> 
  <% flash.each do |name,msg| %>
    <p style="color:green;"><%= msg %></p>
  <% end %>
<% end %>
<table> 
    <tr>Event Details</tr>
    <tr class="table table-dark">
    <td>Event Id</td><td>Name</td><td>Description</td><td>Event Date</td><td>Show</td>  
    <td>Edit</td>
    </tr>
    <% Event.all.each do |s| %>
      <tr>
        <td><%=s.id %></td>
        <td><%=s.name %></td>
        <td><%=s.description %></td>
        <td><%=s.event_date %></td>
        <td><%= button_to 'Show', event_path(s), method: :get%></td>
        <td><%= button_to 'Edit', edit_event_path(s), method: :get%></td>    
      </tr>
    <% end %>
</table>
<%= button_to "Login", login_path,method: :get%>
<%= button_to "Sigup", signup_path,method: :get%>
